{
  "name": "youtube-scraper",
  "version": "1.0.0",
  "description": "Command-line YouTube channel scraper using Bun and Playwright",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "date-fns": "^3.0.0",
    "inquirer": "^12.6.3",
    "playwright": "^1.53.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "bun-types": "latest"
  },
  "keywords": [
    "youtube",
    "scraper",
    "playwright",
    "bun",
    "cli"
  ],
  "author": "rob",
  "license": "MIT"
}
